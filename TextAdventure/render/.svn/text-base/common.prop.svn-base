#title%all
	background_color 0 0 0
	foreground_color 200 200 200
	positioning middle_x 0 0
	font helvetica 64
	margin 0 40 0 40
@button
	background_color 50 50 50
	foreground_color 200 200 200
@button%hover
	foreground_color 250 250 250
@button%pressed
	background_color 250 250 250
	foreground_color 50 50 50
	
#main_button
	positioning middle_x 0 0
	margin 0 20 0 0
	padding 5 5 5 5
	dimension 150 24
#main_button%pressed
	positioning middle_x 5 5
	
#game_list
	positioning middle_x 0 0
	border 5 solid 200 200 200
	
#small_title
	background_color 20 20 20
	foreground_color 200 200 200
	positioning middle_x 0 0
	font helvetica 32
	margin 5 0 0 0