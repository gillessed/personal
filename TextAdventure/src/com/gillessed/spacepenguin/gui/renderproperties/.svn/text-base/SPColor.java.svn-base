package com.gillessed.spacepenguin.gui.renderproperties;

import java.awt.Color;

public class SPColor extends Color {
	private static final long serialVersionUID = -2869818392797353103L;
	
	public SPColor(String[] desc) {
		super(Integer.parseInt(desc[0]), Integer.parseInt(desc[1]), Integer.parseInt(desc[2]));
	}
	public SPColor(int r, int g, int b) {
		super(r, g, b);
	}
}
