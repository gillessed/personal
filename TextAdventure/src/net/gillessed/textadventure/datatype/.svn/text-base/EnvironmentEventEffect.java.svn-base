package net.gillessed.textadventure.datatype;

import net.gillessed.textadventure.dataenums.EnvironmentEffectType;

public class EnvironmentEventEffect extends EventEffect<DataType> {
	private static final long serialVersionUID = -3661237177116949177L;

	private EnvironmentEffectType type;
	
	public EnvironmentEventEffect(Event parent) {
		super(null, parent);
		type = EnvironmentEffectType.values()[0];
	}

	public EnvironmentEffectType getType() {
		return type;
	}

	public void setType(EnvironmentEffectType type) {
		this.type = type;
	}
}
